<Window x:Class="XandO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XandO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ListBox x:Name="lb1" ItemStringFormat="time:{0}" HorizontalAlignment="Center" BorderThickness="0"/>
        <Button Content="1x1" Grid.Row="1" Margin="100,30,100,30" Click="Button_Click" MinWidth="175"/>
        <Button Content="Bot" Grid.Row="2" Margin="100,30,100,30" Click="Button_Click_1" MinWidth="175"/>
    </Grid>
</Window>
